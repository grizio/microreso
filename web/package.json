{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "openapi-generator-cli generate",
    "build": "npm run prebuild && rollup -c",
    "build:prod": "npm run prebuild && npm run build && rm -rf ../server/modules/app/src/main/resources/web && cp -R build ../server/modules/app/src/main/resources/web",
    "dev": "npm run prebuild && rollup -c -w main",
    "dev:initialize": "npm run prebuild && rollup -c -w initialize",
    "dev:main": "npm run prebuild && rollup -c -w main",
    "start": "sirv public",
    "validate": "svelte-check",
    "format": "eslint --fix src"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.0.3",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-typescript": "6.0.0",
    "@tsconfig/svelte": "1.0.10",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "eslint": "7.11.0",
    "rollup": "2.29.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "6.0.1",
    "rollup-plugin-terser": "7.0.2",
    "sirv-cli": "1.0.6",
    "svelte": "3.29.0",
    "svelte-check": "1.0.55",
    "svelte-preprocess": "4.5.1",
    "tslib": "2.0.3",
    "typescript": "4.0.3"
  },
  "dependencies": {}
}
