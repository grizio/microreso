{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "openapi-generator-cli generate",
    "build": "npm run prebuild && rollup -c",
    "build:prod": "npm run prebuild && npm run build && rm -rf ../server/modules/app/src/main/resources/web && cp -R build ../server/modules/app/src/main/resources/web",
    "dev": "npm run prebuild && rollup -c -w main",
    "dev:initialize": "npm run prebuild && rollup -c -w initialize",
    "dev:main": "npm run prebuild && rollup -c -w main",
    "start": "sirv public",
    "validate": "svelte-check",
    "format": "eslint --fix src",
    "storybook": "rimraf node_modules/.cache/storybook && start-storybook -p 8000"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.0.3",
    "@rollup/plugin-commonjs": "16.0.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@rollup/plugin-typescript": "6.1.0",
    "@storybook/addon-a11y": "6.0.28",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-backgrounds": "6.0.28",
    "@storybook/addon-controls": "6.0.28",
    "@storybook/addon-docs": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-knobs": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/addon-storyshots": "6.0.28",
    "@storybook/addon-storysource": "6.0.28",
    "@storybook/addon-viewport": "6.0.28",
    "@storybook/addons": "6.0.28",
    "@storybook/source-loader": "6.0.28",
    "@storybook/svelte": "6.0.28",
    "@tsconfig/svelte": "1.0.10",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "eslint": "7.12.1",
    "rollup": "2.33.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "6.1.0",
    "rollup-plugin-terser": "7.0.2",
    "sirv-cli": "1.0.8",
    "svelte": "3.29.4",
    "svelte-check": "1.1.6",
    "svelte-loader": "2.13.6",
    "svelte-preprocess": "4.5.2",
    "tslib": "2.0.3",
    "typescript": "4.0.5"
  },
  "dependencies": {}
}
